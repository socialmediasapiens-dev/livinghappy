function createTableHeader(e){var t=Ti.UI.createTableViewRow({width:320,backgroundColor:userinfoArray.color,selectionStyle:Ti.UI.iPhone.TableViewCellSelectionStyle.NONE}),a=Ti.UI.createLabel({width:320,height:40,text:e,color:"#FFFFFF",font:{fontSize:18,fontWeight:"bold"},left:20});return t.add(a),t}function createTableAnswer(e){var t=Ti.UI.createTableViewRow({width:320,selectionStyle:Ti.UI.iPhone.TableViewCellSelectionStyle.NONE}),a=Ti.UI.createLabel({width:320,height:40,color:"#8E8C8A",left:20,text:e});return t.add(a),t}Ti.include("../functions.js");var win=Ti.UI.createWindow();designWindow(win,!1,root+"/images/wall/background.png",!1);var wrapperData=[],w=0,scrollWrapper=Ti.UI.createTableView({width:320,height:"auto",separatorStyle:Ti.UI.iPhone.TableViewSeparatorStyle.NONE,separatorColor:"transparent",backgroundColor:"transparent",borderRadius:4,showVerticalScrollIndicator:!1,top:0});win.add(scrollWrapper);var sound=Ti.Media.createSound();sound.url=root+"/files/fresh.mp3",sound.play();var quoteFile=Ti.Filesystem.getFile(Ti.Filesystem.resourcesDirectory,"/files/quotes.xml"),quotestext=quoteFile.read().text,quotes=Ti.XML.parseString(quotestext).getElementsByTagName("quote"),randomnumber=Math.floor(Math.random()*quotes.length),quote=quotes.item(randomnumber).text,file=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,"/userinfo.xml");createFileIfNotExist(file);var xmltext=file.read().text,response=Ti.XML.parseString(xmltext),userinfoArray=[],x=0;userinfoArray.name=response.getElementsByTagName("name").item(0).text,userinfoArray.color=response.getElementsByTagName("color").item(0).text,userinfoArray.q31=response.getElementsByTagName("q31").item(0).text,userinfoArray.q32=response.getElementsByTagName("q32").item(0).text,userinfoArray.q33=response.getElementsByTagName("q33").item(0).text,userinfoArray.q41=response.getElementsByTagName("q41").item(0).text,userinfoArray.q42=response.getElementsByTagName("q42").item(0).text,userinfoArray.q43=response.getElementsByTagName("q43").item(0).text;var tabBarWrapper=Ti.UI.createView({width:250,height:40,top:5,zIndex:999,backgroundColor:"#666666",opacity:.7,borderRadius:4}),winTitle=Ti.UI.createLabel({text:"Welcome "+userinfoArray.name+"!",font:{fontSize:16,fontWeight:"bold"},color:"#FFFFFF",height:40,top:5,borderRadius:4,zIndex:9999}),winTitleRow=Ti.UI.createTableViewRow({width:320,selectionStyle:Ti.UI.iPhone.TableViewCellSelectionStyle.NONE});winTitleRow.add(tabBarWrapper),winTitleRow.add(winTitle),wrapperData[w++]=winTitleRow;var dataQuotes=[],q=0,quoteHeader=Ti.UI.createTableViewRow({width:300,backgroundColor:userinfoArray.color,color:"#FFFFFF",selectionStyle:Ti.UI.iPhone.TableViewCellSelectionStyle.NONE,height:30});dataQuotes[q++]=quoteHeader;var quoteHeaderTitle=Ti.UI.createLabel({width:300,text:"QUOTE",font:{fontSize:16,fontWeight:"bold"},textAlign:"center",color:"#FFFFFF"});quoteHeader.add(quoteHeaderTitle);var quoteRow=Ti.UI.createTableViewRow({width:300,backgroundColor:"#FFFFFF",color:"#666666",selectionStyle:Ti.UI.iPhone.TableViewCellSelectionStyle.NONE,height:Ti.UI.SIZE});dataQuotes[q++]=quoteRow;var quotemark1=Ti.UI.createImageView({image:root+"/images/wall/quotemark1.png",width:16,height:15,left:5,top:5});quoteRow.add(quotemark1);var quotemark2=Ti.UI.createImageView({image:root+"/images/wall/quotemark2.png",width:16,height:15,right:5,top:5});quoteRow.add(quotemark2);var quoteText=Ti.UI.createLabel({text:quote,width:250,height:"auto",font:{fontSize:14},textAlign:"center",top:5});quoteRow.add(quoteText);var quoteBottom=Ti.UI.createTableViewRow({width:300,selectionStyle:Ti.UI.iPhone.TableViewCellSelectionStyle.NONE,height:60}),bubble=Ti.UI.createImageView({image:root+"/images/wall/quote-bubble.png",width:300,height:24,top:0});quoteBottom.add(bubble),dataQuotes[q++]=quoteBottom;var quoteTable=Ti.UI.createTableView({width:300,separatorStyle:Ti.UI.iPhone.TableViewSeparatorStyle.NONE,separatorColor:"transparent",backgroundColor:"transparent",data:dataQuotes,borderRadius:4,scrollable:!1,top:5,bottom:5,height:Ti.UI.SIZE}),quoteRowG=Ti.UI.createTableViewRow({width:320,selectionStyle:Ti.UI.iPhone.TableViewCellSelectionStyle.NONE,height:Ti.UI.SIZE});quoteRowG.add(quoteTable),wrapperData[w++]=quoteRowG;var warningTable=Ti.UI.createTableView({width:320,backgroundColor:"#FFFFFF",separatorColor:"#8E8C8A",height:Ti.UI.SIZE,scrollable:!1,bottom:10}),warningTableRow=Ti.UI.createTableViewRow({width:320,selectionStyle:Ti.UI.iPhone.TableViewCellSelectionStyle.NONE});warningTableRow.add(warningTable),wrapperData[w++]=warningTableRow;var copingTable=Ti.UI.createTableView({width:320,backgroundColor:"#FFFFFF",separatorColor:"#8E8C8A",height:Ti.UI.SIZE}),copingTableRow=Ti.UI.createTableViewRow({width:320,selectionStyle:Ti.UI.iPhone.TableViewCellSelectionStyle.NONE});copingTableRow.add(copingTable),wrapperData[w++]=copingTableRow;var data1=[],x1=0;data1[x++]=createTableHeader("My warning signs"),data1[x++]=createTableAnswer("1. "+userinfoArray.q31),data1[x++]=createTableAnswer("2. "+userinfoArray.q32),data1[x++]=createTableAnswer("3. "+userinfoArray.q33);var data2=[],x2=0;data2[x++]=createTableHeader("My coping strategies"),data2[x++]=createTableAnswer("1. "+userinfoArray.q41),data2[x++]=createTableAnswer("2. "+userinfoArray.q42),data2[x++]=createTableAnswer("3. "+userinfoArray.q43),warningTable.setData(data1),copingTable.setData(data2),scrollWrapper.setData(wrapperData),setTimeout(function(){win.open()},500),setTimeout(function(){sound.stop(),win.close({transition:Ti.UI.iPhone.AnimationStyle.CURL_UP})},15e3),Ti.App.username=userinfoArray.name,Ti.App.userbg=getUserBg(userinfoArray.color);