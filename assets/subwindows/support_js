Ti.include("../functions.js");var win=Ti.UI.currentWindow;designWindow(win,"Support Groups",Ti.App.userbg,!0),createTabGroup(win,0);var header=Ti.UI.createView({width:"100%",height:48,top:52,backgroundColor:"#FFFF"});win.add(header);var indication=Ti.UI.createLabel({width:"90%",height:40,top:2,text:support_groups_indication,color:"#666",textAlign:"center",font:{fontSize:12,fontWeight:"bold"}});header.add(indication);var seperator=Ti.UI.createView({width:320,height:4,bottom:0,backgroundImage:root+"/images/tabbar/colorlines.png"});header.add(seperator);var aWebView=Ti.UI.createWebView({url:support_link,width:"100%",scalesPageToFit:!0,top:100,bottom:53});aWebView.addEventListener("load",function(e){Ti.API.info("webview loaded: "+e.url)}),win.add(aWebView);