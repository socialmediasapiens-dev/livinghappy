Ti.include("../functions.js");var win=Ti.UI.currentWindow;designWindow(win,"Crisis Line",root+"/images/crisis/background.png",!0),createTabGroup(win,0);var welcomeTextWrapper=Ti.UI.createView({width:320,height:185,top:80,backgroundImage:root+"/images/crisis/blacktext-background.png"});win.add(welcomeTextWrapper);var welcomeText=Ti.UI.createWebView({html:sprintf(crisis_welcome_html,Ti.App.username),width:320,height:180,backgroundColor:"transparent",focusable:!1,disableBounce:!0,enableZoomControls:!1,showScrollbars:!1,touchEnabled:!1});welcomeTextWrapper.add(welcomeText);var callButton=Ti.UI.createButton({width:253,height:50,backgroundImage:root+"/images/crisis/dial-button.png",bottom:120*heightRatio});win.add(callButton),callButton.addEventListener("click",function(){var e=crisis_number.replace(/[^0-9]/g,"");e=e.split(" ").join(""),Ti.Platform.openURL("tel:"+e)});