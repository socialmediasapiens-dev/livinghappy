Ti.include("functions.js"),Ti.Platform.displayCaps.platformHeight,Ti.include("notifications.js"),Ti.Network.online||getAlert("You are not connected to internet. Please note that some features may not work properly.");var isUserNew=checkUserNew();if(0==isUserNew){var first=Ti.UI.createWindow({url:"windows/newuser.js"});first.open()}else{var tabGroup=Ti.UI.createTabGroup(),win1=Ti.UI.createWindow({title:"Home",url:"windows/home.js",tabBarHidden:!0,navBarHidden:!0}),tab1=Ti.UI.createTab({window:win1}),win2=Ti.UI.createWindow({title:"Location",url:"windows/location.js",tabBarHidden:!0,navBarHidden:!0}),tab2=Ti.UI.createTab({window:win2}),win3=Ti.UI.createWindow({title:"Reminder",url:"windows/reminder.js",tabBarHidden:!0,navBarHidden:!0}),tab3=Ti.UI.createTab({window:win3}),win4=Ti.UI.createWindow({title:"Settings",url:"windows/settings.js",tabBarHidden:!0,navBarHidden:!0}),tab4=Ti.UI.createTab({window:win4});tabGroup.addTab(tab1),tabGroup.addTab(tab2),tabGroup.addTab(tab3),tabGroup.addTab(tab4),setTimeout(function(){tabGroup.open({transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_RIGHT}),Ti.include("windows/userinfo.js")},3e3),Ti.App.tabgroup=tabGroup}Ti.App.addEventListener("resumed",function(){Ti.API.info("app has resumed from the background")}),Ti.App.addEventListener("pause",function(){Ti.API.info("app was paused from the foreground")});